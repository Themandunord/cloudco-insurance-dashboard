{
  "name": "insurance-bot-dashboard",
  "version": "0.1.0",
  "description": "Insurance Bot Admin Dashboard",
  "main": "index.js",
  "engines": {
    "node": "6.7.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "test:dev-verbose": "npm run test -- --verbose --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "storybook": "start-storybook -p 9001",
    "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "check-coverage": "nyc check-coverage --statements 80 --branches 70 --functions 80 --lines 80"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node_modules/.bin/nyc node --harmony-proxies ./node_modules/.bin/ava",
      "env": {
        "NODE_ENV": "test",
        "NODE_PATH": "src"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IBM-Bluemix/insurance-bot-dashboard.git"
  },
  "author": "Colby Williams <colbycheeze@us.ibm.com> (http://colbycheeze.com)",
  "license": "Apache-2.0",
  "dependencies": {
    " rimraf": "^2.5.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.1.1",
    "cf-deployment-tracker-client": "*",
    "cfenv": "^1.0.3",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "debug": "^3.1.0",
    "es6-promise": "^4.0.5",
    "eslint": "^5.4.0",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.6.3",
    "fs-extra": "^7.0.0",
    "google-map-react": "^1.0.6",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "koa": "^2.0.0-alpha.3",
    "koa-compress": "^3.0.0",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-proxy": "^0.9.0",
    "koa-route": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "material-ui": "^0.20.2",
    "moment": "^2.15.1",
    "mongodb": "^3.1.3",
    "node-sass": "^4.9.3",
    "nodemon": "^1.10.0",
    "normalize.css": "^8.0.0",
    "postcss-loader": "^3.0.0",
    "rc-progress": "^2.0.1",
    "react": "^16.4.2",
    "react-container-query": "^0.11.0",
    "react-dom": "^16.4.2",
    "react-fa": "^5.0.0",
    "react-gravatar": "^2.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-redux": "^4.0.0",
    "react-tap-event-plugin": "^3.0.3",
    "recharts": "^1.1.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "sass-loader": "^7.1.0",
    "sinon": "^6.1.5",
    "socket.io-client": "^2.1.1",
    "style-loader": "^0.22.1",
    "url-loader": "^1.1.1",
    "util": "^0.11.0",
    "watson-developer-cloud": "^3.9.0",
    "webpack": "^4.16.5",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.12.2",
    "whatwg-fetch": "^2.0.4",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "eslint-plugin-import": "^2.14.0",
    "@kadira/react-storybook-decorator-centered": "^1.0.0",
    "@kadira/storybook": "^2.1.0",
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.6",
    "coveralls": "^3.0.2",
    "enzyme": "^3.4.4",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "faker": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.12.0",
    "mock-css-modules": "^1.0.0",
    "nock": "^9.6.1",
    "nodemon": "^1.8.1",
    "nyc": "^12.0.2",
    "react-addons-test-utils": "^15.3.0",
    "redbox-react": "^1.2.10",
    "redux-ava": "^2.1.0",
    "sinon": "^6.1.5"
  },
  "ava": {
    "babel": "inherit",
    "failFast": true,
    "require": [
      "babel-register",
      "./config/ava.config.js"
    ],
    "files": [
      "src/**/*.test.js"
    ],
    "source": [
      "src/**/*.{js,jsx}",
      "!src/**/*.spec.js",
      "!src/**/*.story.js"
    ]
  },
  "nyc": {
    "extension": [
      ".jsx"
    ],
    "include": [
      "src/**/*.{js,jsx}"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.test.js",
      "src/**/*.story.js",
      "src/routes/**/index.js",
      "src/store/*"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
